{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "AzureLogAnalytics"
      },
      {
        "Name": "Console"
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-smog-api",
      "Stage": "PROD"
    }
  }
}