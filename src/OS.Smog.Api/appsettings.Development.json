{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-smog-api",
      "Stage": "DEV"
    }
  },

  "ConnectionStrings": {
    "AzureStorage": "UseDevelopmentStorage=true"
  }
}