{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "LiterateConsole"
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-smog-api",
      "Stage": "DEV"
    }
  }
}