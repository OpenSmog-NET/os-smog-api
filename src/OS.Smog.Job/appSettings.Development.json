{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "LiterateConsole"
      },
      {
        "Name": "Logentries",
        "Args": {
          "token": "68176d24-c475-48c0-9ce0-5c14e682a82c",
          "outputTemplate": "{Timestamp:HH:mm} [{Application}] [{Stage}] [{Level}] {MachineName} ({ThreadId}) {CorrelationId} {Message}{NewLine}{Exception}"
        }
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-smog-job",
      "Stage": "DEV"
    }
  }
}
